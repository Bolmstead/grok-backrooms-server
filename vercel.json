{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.js",
      "continue": true
    },
    {
      "src": "/test",
      "dest": "src/server.js",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "github": {
    "silent": true
  },
  "public": true
}
